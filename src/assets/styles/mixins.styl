ellipsis()
  overflow hidden
  text-overflow ellipsis
  white-space nowrap

ellipsis2()
  display -webkit-box
  overflow hidden
  text-overflow ellipsis
  -webkit-line-clamp 2
  -webkit-box-orient: vertical

centered()
  display flex
  justify-content center
  align-items center

autoMargin()
  width 1200px
  margin 0 auto

imgSize()
  max-width 100%
  max-height 100%

normalFont()
  color #666
  font-size 12px

verticalCenter()
  display flex
  flex-direction column
  align-items center

popupWrap()
  position fixed
  top 0
  left 0
  width 100%
  height 100%
  background rgba(0,0,0,.5)

contentCenter()
  position absolute
  top 50%
  left 50%
  transform translate3d(-50%,-50%,0)
  margin auto
  box-sizing border-box

buttomStyle()
  width 96px
  height 36px
  background-color #F56C6C
  color #FFFFFF
  font-size 14px
  position absolute
  left 50%
  bottom 13px
  transform translateX(-50%)

cardInfo()
  height 56px
  outline none
  border-bottom 1px solid #dee5f3
  line-height 56px
  font-size:14px;
  font-family:MicrosoftYaHei;
  font-weight:400;
  color:rgba(102,102,102,1);
  width 495px
  margin-left 49px
